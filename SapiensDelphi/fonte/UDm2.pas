procedure TDm2.ConsE120IpdAAfterScroll(DataSet: TDataSet);
begin
  CadE120IpdA.Close;
  CadE120IpdA.Parameters.ParamByName('codemp').Value := ConsE120IpdACODEMP.Value;
  CadE120IpdA.Parameters.ParamByName('codfil').Value := ConsE120IpdACODFIL.Value;
  CadE120IpdA.Parameters.ParamByName('numped').Value := ConsE120IpdANUMPED.Value;
  CadE120IpdA.Parameters.ParamByName('seqipd').Value := ConsE120IpdASEQIPD.Value;
  CadE120IpdA.Open;

  FAssistencia.EdCodPro.Text := ConsE120IpdACODPRO.Value;
  FAssistencia.LDsCodPro.Caption := ConsE120IpdADESPRO.Value;
  FAssistencia.EdCodDer.Text := ConsE120IpdACODDER.Value;
  FAssistencia.EdTnsProI.Text := ConsE120IpdATNSPRO.Value;
  FAssistencia.EdDatFab.Date := CadE120IpdAUSU_DATFAB.Value;
  FAssistencia.EdDatSol.Date := CadE120IpdAUSU_DATSOL.Value;
  FAssistencia.EdDatPer.Date := CadE120IpdAUSU_DATPER.Value;
  FAssistencia.MArgSol.Text := CadE120IpdAUSU_ARGSOL.Value;
  FAssistencia.MDesPer.Text := CadE120IpdAUSU_DESPER.Value;
  FAssistencia.MMatUti.Text := CadE120IpdAUSU_MATUTI.Value;
  FAssistencia.EdVlrPro.Value := ConsE120IpdAPREUNI.AsCurrency;
  FAssistencia.EdVlrAst.Value := CadE120IpdAUSU_VLRPRO.AsCurrency;
  if CadE120IpdAUSU_INDEV.Value = 'DEVIDA' then
     FAssistencia.EdInDev.ItemIndex := 1
  else
  if CadE120IpdAUSU_INDEV.Value = 'INDEVIDA' then
     FAssistencia.EdInDev.ItemIndex := 2
  else
     FAssistencia.EdInDev.ItemIndex := 0;

  //consulta a cor do produto
  ConsGeral.Close;
  ConsGeral.SQL.Clear;
  ConsGeral.SQL.Add('select * from usu_t120ipd where usu_codemp = :codemp and');
  ConsGeral.SQL.Add('usu_codfil = :codfil and');
  ConsGeral.SQL.Add('usu_numped = :numped and');
  ConsGeral.SQL.Add('usu_seqipd = :seqipd');
  ConsGeral.SQL.Add('order by usu_seqite');
  ConsGeral.Parameters.ParamByName('codemp').Value := ConsE120IpdACODEMP.Value;
  ConsGeral.Parameters.ParamByName('codfil').Value := ConsE120IpdACODFIL.Value;
  ConsGeral.Parameters.ParamByName('numped').Value := ConsE120IpdANUMPED.Value;
  ConsGeral.Parameters.ParamByName('seqipd').Value := ConsE120IpdASEQIPD.Value;
  ConsGeral.Open;
  FAssistencia.EdCor01.Text := ConsGeral.FieldByName('usu_cor001').AsString;
  FAssistencia.EdCor02.Text := ConsGeral.FieldByName('usu_cor002').AsString;
end;

