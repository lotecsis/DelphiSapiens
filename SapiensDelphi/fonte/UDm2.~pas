uses UDm;

{$R *.dfm}

procedure TDm2.ConsE120IpdAfterScroll(DataSet: TDataSet);
begin
    ConsUsu_T120Ipd.Close;
    ConsUsu_T120Ipd.Parameters.ParamByName('codfil').Value := ConsE120IpdCODFIL.Value;
    ConsUsu_T120Ipd.Parameters.ParamByName('numped').Value := ConsE120IpdNUMPED.Value;
    ConsUsu_T120Ipd.Parameters.ParamByName('seqipd').Value := ConsE120IpdSEQIPD.Value;
    ConsUsu_T120Ipd.Open;
end;

end.
