procedure TDmOra.ComparativoComprasCliAfterScroll(DataSet: TDataSet);
begin
 FComparaComprasClientes.EdTotFatAnt.Value := ComparativoComprasCliTOTESTOFANT.AsCurrency +
                                              ComparativoComprasCliTOTCOLCHANT.AsCurrency;

 FComparaComprasClientes.EdTotFatAtu.Value := ComparativoComprasCliTOTESTOFATU.AsCurrency +
                                              ComparativoComprasCliTOTCOLCHATU.AsCurrency;

 FComparaComprasClientes.EdTotPedAbeAtu.Value := ComparativoComprasCliTOTPEDABEESTOF.AsCurrency +
                                                 ComparativoComprasCliTOTPEDABECOLCH.AsCurrency;

 FComparaComprasClientes.EdTotFatPed.Value := ComparativoComprasCliTOTESTOFATU.AsCurrency +
                                                ComparativoComprasCliTOTCOLCHATU.AsCurrency +
                                                ComparativoComprasCliTOTPEDABEESTOF.AsCurrency +
                                                ComparativoComprasCliTOTPEDABECOLCH.AsCurrency;

 FComparaComprasClientes.EdTotEstofAtu.Value := ComparativoComprasCliTOTESTOFATU.AsCurrency +
                                                ComparativoComprasCliTOTPEDABEESTOF.AsCurrency;

 FComparaComprasClientes.EdTotColchAtu.Value := ComparativoComprasCliTOTCOLCHATU.AsCurrency +
                                                ComparativoComprasCliTOTPEDABECOLCH.AsCurrency;


 FComparaComprasClientes.EdDifTotal.Value := FComparaComprasClientes.EdTotFatPed.Value -
                                             FComparaComprasClientes.EdTotFatAnt.Value;

  //faz a consulta das vendas por mes
  ComparativoComprasCliMes.Close;
  ComparativoComprasCliMes.Parameters.ParamByName('codcli').Value := ComparativoComprasCliCODCLI.Value;
  ComparativoComprasCliMes.Open;

  //CONSULTA OS TITULOS
  ConsE301Tcr.Close;
  ConsE301Tcr.Parameters.ParamByName('DataA').Value := date;
  ConsE301Tcr.Parameters.ParamByName('DataV').Value := date;
  ConsE301Tcr.Parameters.ParamByName('CodCli').Value := ComparativoComprasCliCODCLI.Value;
  ConsE301Tcr.Open;

  //consulta o ultimo pedido
  ConsSql.Close;
  ConsSql.SQL.Clear;
  ConsSql.SQL.Add('SELECT DATUPE, VLRUPE');
  ConsSql.SQL.Add('FROM E085HCL, E070FIL');
  ConsSql.SQL.Add('WHERE E085HCL.CODEMP = 1  AND');
  ConsSql.SQL.Add('E085HCL.CODCLI = :codcli  AND');
  ConsSql.SQL.Add('E070FIL.CODEMP = E085HCL.CODEMP AND');
  ConsSql.SQL.Add('E070FIL.CODFIL = E085HCL.CODFIL AND');
  ConsSql.SQL.Add('0=0 ORDER BY DATUPE DESC');
  ConsSql.Parameters.ParamByName('codcli').Value := ComparativoComprasCliCODCLI.Value;
  ConsSql.Open;
  FComparaComprasClientes.EdDatUpe.Date := ConsSql.FieldByName('DATUPE').AsDateTime;
  FComparaComprasClientes.EdVlrUpe.Value := ConsSql.FieldByName('VLRUPE').AsCurrency;

  //consulta a ultima fatura
  ConsSql.Close;
  ConsSql.SQL.Clear;
  ConsSql.SQL.Add('SELECT DATUFA, VLRUFA, ULTNFV, ULTSNF');
  ConsSql.SQL.Add('FROM E085HCL, E070FIL');
  ConsSql.SQL.Add('WHERE E085HCL.CODEMP = 1  AND');
  ConsSql.SQL.Add('E085HCL.CODCLI = :codcli  AND');
  ConsSql.SQL.Add('E070FIL.CODEMP = E085HCL.CODEMP AND');
  ConsSql.SQL.Add('E070FIL.CODFIL = E085HCL.CODFIL AND');
  ConsSql.SQL.Add('0=0 ORDER BY DATUFA DESC	');
  ConsSql.Parameters.ParamByName('codcli').Value := ComparativoComprasCliCODCLI.Value;
  ConsSql.Open;
  FComparaComprasClientes.EdDatUfa.Date := ConsSql.FieldByName('DATUFA').AsDateTime;
  FComparaComprasClientes.EdVlrUfa.Value := ConsSql.FieldByName('VLRUFA').AsCurrency;
  FComparaComprasClientes.EdUltNfv.Text := IntToStr(ConsSql.FieldByName('ULTNFV').AsInteger);
  FComparaComprasClientes.EdUltSnf.Text := ConsSql.FieldByName('ULTSNF').AsString;

  ConsSql.Close;
  ConsSql.SQL.Clear;
  ConsSql.SQL.Add('SELECT DATUPG, VLRUPG, QTDPGT');
  ConsSql.SQL.Add(' FROM E085HCL, E070FIL');
  ConsSql.SQL.Add(' WHERE E085HCL.CODEMP = 1  AND');
  ConsSql.SQL.Add(' E085HCL.CODCLI = :codcli  AND');
  ConsSql.SQL.Add(' E070FIL.CODEMP = E085HCL.CODEMP AND');
  ConsSql.SQL.Add(' E070FIL.CODFIL = E085HCL.CODFIL AND');
  ConsSql.SQL.Add(' 0=0 ORDER BY DATUPG DESC  	');
  ConsSql.Parameters.ParamByName('codcli').Value := ComparativoComprasCliCODCLI.Value;
  ConsSql.Open;
  FComparaComprasClientes.EdDatUpg.Date := ConsSql.FieldByName('DATUPG').AsDateTime;
  FComparaComprasClientes.EdVlrUpg.Value := ConsSql.FieldByName('VLRUPG').AsCurrency;

  ConsSql.Close;
  ConsSql.SQL.Clear;
  ConsSql.SQL.Add('SELECT VLRATR,MAIATR');
  ConsSql.SQL.Add('FROM E085HCL');
  ConsSql.SQL.Add('WHERE');
  ConsSql.SQL.Add('E085HCL.CODEMP = 1 AND');
  ConsSql.SQL.Add('E085HCL.CODCLI = :codcli');
  ConsSql.SQL.Add('ORDER BY E085HCL.MAIATR DESC');
  ConsSql.Parameters.ParamByName('codcli').Value := ComparativoComprasCliCODCLI.Value;
  ConsSql.Open;
  FComparaComprasClientes.EdMaiAtr.Value := ConsSql.FieldByName('MAIATR').AsInteger;
  FComparaComprasClientes.EdVlrAtr.Value := ConsSql.FieldByName('VLRATR').AsCurrency;
 end;

procedure TDmOra.ConsE504CapAgruCalcFields(DataSet: TDataSet);
var vnVlrAju : Double;
begin
  if (ConsE504CapAgruCODTNS.Value = '90593') or (ConsE504CapAgruCODTNS.Value = '90595') or (ConsE504CapAgruCODTNS.Value = '90590') then
     begin
       ConsE504CapAgruTOTAL_BASE.AsFloat := ConsE504CapAgruTOTAL_BASE.AsFloat * (-1);
       ConsE504CapAgruTOTAL_COMISSAO.AsFloat := ConsE504CapAgruTOTAL_COMISSAO.AsFloat * (-1);
     end;

   {if (ConsE504CapAgruCODTNS.Value <> '90593') and (ConsE504CapAgruCODTNS.Value <> '90595') and (ConsE504CapAgruCODTNS.Value <> '90590') then
       begin
          ConsE504CapAgruvnVlrReajuste.AsFloat := ConsE504CapAgruTOTAL_BASE.AsFloat * (ConsE504CapAgruUSU_PERAPL.AsFloat/100);
       end
   else
       begin
          ConsE504CapAgruvnVlrReajuste.AsFloat := 0;
       end; }

   ConsE504CapAgruvnVlrReajuste.AsFloat := ConsE504CapAgruTOTAL_BASE.AsFloat * (ConsE504CapAgruUSU_PERAPL.AsFloat/100);

   //se foi informado ajuste na comissao para o mes faz o calculo
   if not ConsUSU_TAjuCom.IsEmpty then
      begin
        if (ConsE504CapAgruvnVlrReajuste.AsFloat <> 0) and (FCalculoAjusteComissoesAgru.chkAjustar.Checked) then
           begin
              //fao o calculo pra ver quanto o valor de ajuste representa no total que tem que ajustar
              if FCalculoAjusteComissoesAgru.edtTotalAjuste1_101.Value > 0 then
                 begin
                   ConsE504CapAgruvnPerRepresenta.AsFloat := (ConsE504CapAgruvnVlrReajuste.AsFloat * 100) / FCalculoAjusteComissoesAgru.edtTotalAjuste1_101.Value;
                 end;
              {vnVlrAju := ((ConsE504CapAgruvnVlrReajuste.AsFloat * 100) / DmOra.ConsUSU_TAjuComUSU_TOTAJU.AsFloat);
              vnVlrAju := DmOra.ConsUSU_TAjuComUSU_VLRAJU.AsFloat * (vnVlrAju / 100);}

              vnVlrAju := (ConsE504CapAgruvnPerRepresenta.AsFloat / 100) * DmOra.ConsUSU_TAjuComUSU_VLRAJU.AsFloat;
              ConsE504CapAgruvnVlrReajuste.AsFloat := ConsE504CapAgruvnVlrReajuste.AsFloat + vnVlrAju;
           end;
      end;


  //ConsE504CapAgruvnTotalComissao.AsFloat := ConsE504CapAgruTOTAL_COMISSAO.AsFloat + ConsE504CapAgruvnVlrReajuste.AsFloat;
  if (ConsE504CapAgruCODTNS.Value <> '90582') then
     begin
       ConsE504CapAgruvnDezeAvos.AsFloat := ConsE504CapAgruTOTAL_COMISSAO.AsFloat / 12;
     end;
  ConsE504CapAgruvnTotalComissao.AsFloat := ConsE504CapAgruTOTAL_COMISSAO.AsFloat + ConsE504CapAgruvnVlrReajuste.AsFloat + ConsE504CapAgruvnDezeAvos.AsFloat;

  ConsE504CapAgruvaNumTit.AsString := Copy(ConsE504CapAgruNUMNFV.AsString,1,6);
  ConsE504CapAgruvnTotalPercentual.AsFloat := ConsE504CapAgruPERCOM.AsFloat + ConsE504CapAgruUSU_PERAPL.AsFloat;
end;

